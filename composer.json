{
    "name": "camaleaun/run-command",
    "description": "Run scripts",
    "type": "wp-cli-package",
    "homepage": "https://github.com/camaleaun/run-command",
    "license": "MIT",
    "authors": [
        {
            "name": "Gilberto Tavares",
            "email": "camaleaun@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "files": [ "run-script.php" ]
    },
    "require": {
        "php": "^5.4 || ^7.0",
        "wp-cli/wp-cli": "^2"
    },
    "require-dev": {
        "wp-cli/wp-cli-tests": "^2.1"
    },
    "extra": {
        "commands": [
            "run"
        ]
    }
}
